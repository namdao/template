{
  "presets": [
    "module:metro-react-native-babel-preset"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console"
      ]
    },
    "development": {
      "presets": [
        [
          "module:metro-react-native-babel-preset",
          {
            "development": true
          }
        ]
      ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        // "cwd": "babelrc",
        "root": [
          "./src"
        ],
        "alias": {
          "api": "./src/api",
          "assets": "./src/assets",
          "storeConfig": "./src/storeConfig",
          "scenes": "./src/scenes",
          "sagas": "./src/sagas",
          "reducer": "./src/reducer",
          "utils": "./src/utils",
          "test": "./test",
          "underscore": "lodash"
        },
        "extensions": [
          ".android.js",
          ".ios.js",
          ".js",
          ".json"
        ]
      }
    ]
  ],
  "retainLines": true
}